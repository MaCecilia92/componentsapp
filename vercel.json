{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm install --ignore-platform",
    "framework": "vite",
    "headers": [
      {
        "source": "/assets/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }